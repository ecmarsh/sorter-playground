{
  "name": "sorter",
  "version": "1.0.0",
  "description": "Demo for generic class organization",
  "main": "index.js",
  "scripts": {
    "start": "concurrently npm:start:*",
    "start:compile": "tsc -w",
    "start:run": "nodemon build/index.js"
  },
  "license": "ISC",
  "dependencies": {
    "concurrently": "^4.1.1",
    "nodemon": "^1.19.1"
  },
  "nodemonConfig": {
    "ignore": [
      "node_modules/*",
      "*.ts"
    ],
    "delay": "1000"
  }
}
